<launch>
 
  <include file="$(find ur10_moveit_config)/launch/ur10_moveit_planning_execution.launch">
  <arg name ="sim" value="true"/>
</include>

  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <param name="/use_gui" value="true"/>
  </node>

  <node name="test_node" pkg="ur10_trials" type="test_node" respawn="false" output="screen">
  </node>
  
	<arg name="scene_file" default="qualifier_2b/config/ariac.scene"/> 
	<node name = "moveit_publish_scene_from_text" pkg= "moveit_ros_planning" type = "moveit_publish_scene_from_text" args= "$(arg scene_file)"/>

</launch>